{
    "manifest_version": 3,
    "name": "Test Tracker",
    "version": "1.0",
    "description": "Displays test results",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon48.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon48.png",
      "48": "icon48.png",
      "128": "icon48.png"
    }
  }
  